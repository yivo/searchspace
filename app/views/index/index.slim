.search
  .search-wrapper
    .search-header
      .search-title SEARCH SPACE
      .search-slogan One website to search them all.
    .search-body
      input.search-field#search-phrase[
        type          ='text'
        autofocus     =true
        spellcheck    ='true'
        autocomplete  ='off'
        autocapitalize='off'
        autocorrect   ='off'
        placeholder   ='I am looking for...'
        value         =params[:phrase]
      ]
      #search-outlet
